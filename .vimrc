execute pathogen#infect()

""""""""""""""""""""""""""""""
" 01. General                "
""""""""""""""""""""""""""""""
set nocompatible

""""""""""""""""""""""""""""""
" 02. Events                 "
""""""""""""""""""""""""""""""
filetype plugin indent on

""""""""""""""""""""""""""""""
" 03. Theme/Colors           "
""""""""""""""""""""""""""""""
set t_Co=256
syntax enable
set background=dark
colorscheme solarized

""""""""""""""""""""""""""""""
" 04. Vim UI                 "
""""""""""""""""""""""""""""""
set number
set cul
set ignorecase
set ruler
"set numberwidth=6

""""""""""""""""""""""""""""""
" 05. Text Formatting/Layout "
""""""""""""""""""""""""""""""
set expandtab
set tabstop=4
set softtabstop=4
set shiftwidth=4
