[include]
    path = ~/.gitconfig.local
[fetch]
    prune = true
[push]
    default = current
[core]
    excludesfile = ~/.gitignore
    editor = nvim
[color]
    ui = auto
[pull]
    rebase = false
[init]
    defaultBranch = main
[filter "lfs"]
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
    clean = git-lfs clean -- %f

